{
  "name": "eslint-config-happy",
  "version": "0.0.0",
  "description": "Shareable ESLint config for happier developers",
  "keywords": [
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "prettier",
    "typescript",
    "opinionated",
    "hint",
    "jscs",
    "jshint",
    "tslint",
    "lint",
    "policy",
    "quality",
    "simple",
    "neaf",
    "happy",
    "happy style",
    "style",
    "style checker",
    "style linter",
    "verify"
  ],
  "homepage": "https://github.com/arsnl/eslint-config-happy",
  "bugs": {
    "url": "https://github.com/arsnl/eslint-config-happy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/arsnl/eslint-config-happy.git"
  },
  "license": "MIT",
  "author": {
    "name": "Neaf",
    "email": "ahoy@neaf.co",
    "url": "https://neaf.co"
  },
  "contributors": [
    {
      "name": "Marc-AndrÃ© Arseneault",
      "email": "marc-andre@arsnl.co",
      "url": "https://arsnl.co"
    }
  ],
  "main": "index.js",
  "bin": {
    "eslint-config-happy": "bin/cli.js"
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf ./bin/",
    "dev": "npm run clean && rollup -c -w",
    "fix": "npm run lint -- --fix && npm run format",
    "format": "prettier --write --loglevel warn .",
    "lint": "eslint .",
    "release": "npm run build && np",
    "test": "npm run lint && npm run test:format && npm run test:unit",
    "test:format": "prettier --check --loglevel warn .",
    "test:unit": "nyc ava"
  },
  "ava": {
    "files": [
      "**/*.test.*"
    ]
  },
  "nyc": {
    "all": true,
    "exclude": [
      ".eslintrc.*",
      "rollup.config.*",
      "**/*.test.*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "confusing-browser-globals": "^1.0.10",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-regexp": "^0.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^24.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "ava": "^3.15.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "eslint": ">=7.8.0",
    "global-dirs": "^2.1.0",
    "jest": "^26.6.3",
    "np": "^7.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "resolve-from": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "eslint": ">=7.17.0",
    "prettier": ">=2.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "yarn": false
  }
}
